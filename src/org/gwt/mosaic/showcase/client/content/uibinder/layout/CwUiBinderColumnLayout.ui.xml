<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:m='urn:import:org.gwt.mosaic.ui.client'
	xmlns:l='urn:import:org.gwt.mosaic.ui.client.layout' xmlns:f='urn:import:org.gwt.mosaic.forms.client.layout'>

	<l:LayoutPanel ui:field='layoutPanel'>
		<l:ColumnLayout>
			<l:ColumnLayoutData flexibility="2">
				<m:ToolButton text="ToolButton1" />
			</l:ColumnLayoutData>
			<m:ToolButton text="ToolButton2" />
			<l:ColumnLayoutData flexibility="3">
				<m:ToolButton text="ToolButton3" />
			</l:ColumnLayoutData>
			<l:ColumnLayoutData preferredWidth="40%">
				<m:ToolButton text="ToolButton4" />
			</l:ColumnLayoutData>
			<m:ToolButton text="ToolButton5" />
		</l:ColumnLayout>
	</l:LayoutPanel>

</ui:UiBinder>