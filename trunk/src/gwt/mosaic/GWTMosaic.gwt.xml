<?xml version="1.0" encoding="UTF-8"?>

<module rename-to='gwtmosaic'>

	<!-- Inherit the core Web Toolkit stuff. -->
	<inherits name='com.google.gwt.user.User' />

	<!-- Other module inherits -->
	<inherits name='gwt.mosaic.BeanAdapter' />
	<inherits name='gwt.mosaic.ComputedStyle' />
	
	<!-- Inherit the default GWTMosaic style sheet. -->
	<stylesheet src="GWTMosaic.css"/>

	<!-- Specify the paths for translatable code -->
	<source path='client' />
	<source path='shared' />

</module>
